<appian-plugin name="Fast Excel"
	key="fast-excel">
	<plugin-info>
		<description>Plugins used for Excel Operations - ExportSQLToExcel
		</description>
		<vendor name="Government Of Singapore"
			url="https://www.tech.gov.sg" />
		<version>1.1.4</version>
		<application-version min="23.4" />
	</plugin-info>
	<!-- Data Type -->
	<datatype key="SQLSheetData" name="Input Datatype for Fast-Excel Plugin">
		<class>com.cts.fastexcel.datatype.SQLSheetData</class>
	</datatype>
	<!-- Smart Service -->
	<smart-service name="Write SQL to Excel (Fast Excel)" key="writeSQLToExcelV1"
		class="com.cts.fastexcel.smartservice.WriteSQLToExcel" />
	<!-- Function Category -->
	<function-category key="fastExcelFunctions"
					   name="Fast Excel Functions" />

	<!-- Functions -->
	<function name="Read Excel Using Fast Excel" key="readUsingFastExcelV1"
			  class="com.cts.fastexcel.function.ReadExcelFile" />
</appian-plugin>